{
  "name": "LMS Playback Rate Extension",
  "version": "0.9",
  "description": "LMS상의 사이버 강의 영상을 모두 시청하지 않아도 속도를 조절할 수 있도록 하는 크롬 익스텐션입니다.",
  "manifest_version": 2,

  "browser_action": {
    "default_icon": {
      "16": "images/favicon-16x16.png",
      "32": "images/favicon-32x32.png",
      "48": "images/favicon-48x48.png",
      "128": "images/favicon-128x128.png"
    }
  },
  "icons": {
    "16": "images/favicon-16x16.png",
    "32": "images/favicon-32x32.png",
    "48": "images/favicon-48x48.png",
    "128": "images/favicon-128x128.png"
  },

  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.sch.ac.kr/*",
        "https://*.sch.ac.kr/*"
      ],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["index.js"]
}